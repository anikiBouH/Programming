#include <stdio.h>
void multiplication_table(int x, int y);
void printStars3(int stars);
void printStars31(int stars)

int main()
{

	multiplication_table(12,13);
	printf("\n");
	printStars3(10);
	printf("\n");
	return 0;
}
void printStars31(int stars)
{
	int i;
	int j;
	for (i=0  ; i<stars; i++)
	{
		for (j=0; j<stars; j++)
		{
			if ((i == j)||(j==0)||(j==stars-1)||(i==0)||(i==stars-1)||(j>i))
			{
				printf("*");
			}
			else
			{
				printf(" ");
			}

		}
		printf("\n");
	}
}

void multiplication_table(int x, int y)
{
	int i,j;

		for( i = 1; i<=y; i++)
		{
			for( j = 1; j<=x; j++)
			{
				printf("%3d  x%3d=%3d\t",i,j,i*j);
			}
			printf("\n");
		}

}

void printStars3(int stars)
{
	int i;
	int j;
	for (i=0  ; i<stars; i++)
	{
		for (j=0; j<stars; j++)
		{
			if ((i == j)||(j==0)||(j==stars-1)||(i==0)||(i==stars-1)||(j<i))
			{
				printf("*");
			}
			else
			{
				printf(" ");
			}

		}
		printf("\n");
	}
}
