#include <stdio.h>.

struct person
{
    int age;
    char name;
};
int main(void)
{
    struct person tom = {23, "Tom"};
    printf("Enter name: ");
    scanf("%s", &tom.name);
    printf("\nEnter age: ");
    scanf("%d", &tom.age);
    printf("\nName:%s \t Age: %d", tom.name, tom.age);
    return 0;
}
