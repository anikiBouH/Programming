void multiplication_table()
{
	int i,j;
	for(j = 1 ; j <= 10; j++)
	{
		for(i = 1; i <= 10;i++)
		{
		printf("%dx%d=%d\t",j,i,i*j);
		}
		printf("\n");
	}
}
